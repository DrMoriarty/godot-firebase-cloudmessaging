{
  "name": "firebase-cloud-messaging",
  "display_name": "Firebase Cloud Messaging",
  "description": "Firebase Cloud Messaging plugin. Recommended for use with FirebaseAnalytics.",
  "version": "0.2.1",
  "godot_version": "3.3.0",
  "license": "MIT",
  "url": "https://github.com/DrMoriarty/godot-firebase-cloudmessaging",
  "category": "misc",
  "author": {
    "name": "DrMoriarty",
    "url": "https://github.com/DrMoriarty",
    "donate": "https://ko-fi.com/drmoriarty"
  },
  "dependencies": [
    "firebase-core", "firebase-analytics"
  ],
  "files": {
    "scripts": "scripts"
  },
  "autoload": {
    "messaging": "*res://scripts/messaging.gd"
  },
  "platform_android": {
    "files": {
      "android-plugin/FirebaseCloudMessaging.gdap": "android/plugins/",
      "android-plugin/build/outputs/aar/FirebaseCloudMessaging.release.aar": "android/plugins/"
    }
  },
  "platform_ios": {
    "files": {
      "ios-plugin/bin/firebase-cloud-messaging.debug.xcframework": "ios/plugins/firebase-cloud-messaging/firebase-cloud-messaging.debug.xcframework",
      "ios-plugin/bin/firebase-cloud-messaging.release.xcframework": "ios/plugins/firebase-cloud-messaging/firebase-cloud-messaging.release.xcframework",
      "ios-plugin/firebase-cloud-messaging.gdip": "ios/plugins/firebase-cloud-messaging/",
      "ios-plugin/FirebaseInstanceID.xcframework": "ios/plugins/firebase-cloud-messaging/FirebaseInstanceID.xcframework",
      "ios-plugin/FirebaseMessaging.xcframework": "ios/plugins/firebase-cloud-messaging/FirebaseMessaging.xcframework"
    }
  }
}
